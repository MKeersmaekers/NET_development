@{
    string name = ViewData["name"].ToString();
    int counter = ViewBag.counter; //(int)ViewData["counter"]
}

<h2>Detail</h2>
<p>@name</p>
<p>@counter</p>

<ul>
    @for (int i = 0; i < counter; i++)
    {
        <li>@name</li>
    }
</ul>