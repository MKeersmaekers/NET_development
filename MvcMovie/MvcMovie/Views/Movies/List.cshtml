@model List<Movie>

<h2>List movies</h2>

<table>
    <tr>
        <th>@(Html.DisplayNameFor(model => model[0].Title))</th>
        <th>@(Html.DisplayNameFor(model => model[0].ReleaseDate))</th>
        <th>@(Html.DisplayNameFor(model => model[0].Genre))</th>
    </tr>
    @foreach (Movie movie in Model)
    {
    <tr>
            <td>
                <a asp-controller="Movies" asp-action="Details" asp-route-id="@movie.MovieID">
                    @(Html.DisplayFor(modelItem => movie.Title))
                </a>
            </td>
        <td>@(Html.DisplayFor(rd => movie.ReleaseDate))</td>
        <td>@(Html.DisplayFor(rd => movie.Genre))</td>
    </tr>
    }

</table>